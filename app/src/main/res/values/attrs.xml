<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ProgressView">
        <attr name="color_main" format="color" />
        <attr name="color_secondary" format="color" />
        <attr name="square_size" format="dimension" />
        <attr name="width_between_rect" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleMenu">
        <attr name="radius" format="dimension" />
        <attr name="count_of_sectors" format="integer" />
        <attr name="main_stroke" format="dimension" />
        <attr name="secondary_stroke" format="dimension" />
        <attr name="color_menu_main" format="color" />
        <attr name="color_menu_secondary" format="color" />
        <attr name="color_background" format="color" />
    </declare-styleable>

</resources>